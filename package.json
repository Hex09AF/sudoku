{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "dev": "concurrently \"npm run sass\" \"remix watch\" \"npm run server:dev\"",
    "start:build": "remix-serve build",
    "typecheck": "tsc -b",
    "sass": "sass --watch app/:app/",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "server:dev": "cross-env NODE_ENV=development nodemon server/index.js --watch build/index.js",
    "push": "npx prisma db push"
  },
  "prisma": {
    "seed": "node --require esbuild-register prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^4.8.0",
    "@remix-run/node": "^1.9.0",
    "@remix-run/react": "^1.9.0",
    "@remix-run/serve": "^1.9.0",
    "bcryptjs": "^2.4.3",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "isbot": "^3.6.5",
    "lodash.debounce": "^4.0.8",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "socket.io": "^4.5.4",
    "socket.io-client": "^4.5.4"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.9.0",
    "@remix-run/eslint-config": "^1.9.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "concurrently": "^7.6.0",
    "esbuild-register": "^3.4.2",
    "eslint": "^8.27.0",
    "nodemon": "^2.0.20",
    "prisma": "^4.8.0",
    "sass": "^1.57.1",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=14"
  }
}
